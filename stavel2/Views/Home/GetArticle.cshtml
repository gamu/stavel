@using stavel2.Models
@model  Material
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = Model.Title ?? string.Empty;
    ViewBag.Description = Model.Description ?? string.Empty;
    ViewBag.Keywords = Model.Keywords ?? string.Empty;    
}
<header>
    <h2>@Model.Title</h2>
</header>
<article>
    <section>
        @Html.Raw(Model.Content)
    </section>
</article>
